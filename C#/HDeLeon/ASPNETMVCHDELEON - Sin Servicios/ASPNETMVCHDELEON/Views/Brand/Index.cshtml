@model List<Brand>
<h1> Marcas </h1>
<table class="table">
    <thead> 
        <tr>
            <th>Nombre</th>
        </tr>
    </thead>
        <tbody> 
            @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(m=>item.Name)</td>
            </tr>

        }
        </tbody>
</table>